@article{trauth-goik_black_2023,
	title = {Black or Fifty Shades of Grey? The Power and Limits of the Social Credit Blacklist System in China},
	volume = {32},
	issn = {1067-0564},
	url = {https://doi.org/10.1080/10670564.2022.2128638},
	doi = {10.1080/10670564.2022.2128638},
	shorttitle = {Black or Fifty Shades of Grey?},
	abstract = {Punishment from the state can rely on formal state apparatuses, but also the mobilization and co-option of the deviant’s own social connections to enhance the power of social control. This study utilizes a mixed-method design based on 30 interviews and a national survey to examine how such ‘relational punishment’ operates in China today as part of the nation-wide Social Credit Blacklist System. The authors first trace the history of blacklisting as a governance tool. The article then illustrates how the state’s symbolic campaign encourages the ostracization of blacklisted people. However, this power has its limits. People commonly differentiate the character of blacklisted people with contextual and relational information, constructing alternative meanings for individuals thus labelled, therefore undermining the reach and influence of the Blacklist System.},
	pages = {1017--1033},
	number = {144},
	journaltitle = {Journal of Contemporary China},
	author = {Trauth-Goik, Alexander and Liu, Chuncheng},
	urldate = {2024-12-05},
	date = {2023-11-02},
	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/10670564.2022.2128638},
}

@inproceedings{engelmann_blacklists_2021,
	location = {Virtual Event {USA}},
	title = {Blacklists and Redlists in the Chinese Social Credit System: Diversity, Flexibility, and Comprehensiveness},
	isbn = {978-1-4503-8473-5},
	url = {https://dl.acm.org/doi/10.1145/3461702.3462535},
	doi = {10.1145/3461702.3462535},
	shorttitle = {Blacklists and Redlists in the Chinese Social Credit System},
	abstract = {The Chinese Social Credit System ({SCS}) is a novel digital sociotechnical credit system. The {SCS} aims to regulate societal behavior by reputational and material devices. Scholarship on the {SCS} has offered a variety of legal and theoretical perspectives. However, little is known about its actual implementation. Here, we provide the first comprehensive empirical study of digital blacklists (listing “bad” behavior) and redlists (listing “good” behavior) in the Chinese {SCS}. Based on a unique data set of reputational blacklists and redlists in 30 Chinese provincial-level administrative divisions ({ADs}), we show the diversity, flexibility, and comprehensiveness of the {SCS} listing infrastructure. First, our results demonstrate that the Chinese {SCS} unfolds in a highly diversified manner: we find differences in accessibility, interface design and credit information across provincial-level {SCS} blacklists and redlists. Second, {SCS} listings are flexible. During the {COVID}-19 outbreak, we observe a swift addition of blacklists and redlists that helps strengthen the compliance with coronavirus-related norms and regulations. Third, the {SCS} listing infrastructure is comprehensive. Overall, we identify 273 blacklists and 154 redlists across provincial-level {ADs}. Our blacklist and redlist taxonomy highlights that the {SCS} listing infrastructure prioritizes law enforcement and industry regulations. We also identify redlists that reward political and moral behavior. Our study substantiates the enormous scale and diversity of the Chinese {SCS} and puts the debate on its reach and societal impact on firmer ground. Finally, we initiate a discussion on the ethical dimensions of data-driven research on the {SCS}.},
	eventtitle = {{AIES} '21: {AAAI}/{ACM} Conference on {AI}, Ethics, and Society},
	pages = {78--88},
	booktitle = {Proceedings of the 2021 {AAAI}/{ACM} Conference on {AI}, Ethics, and Society},
	publisher = {{ACM}},
	author = {Engelmann, Severin and Chen, Mo and Dang, Lorenz and Grossklags, Jens},
	urldate = {2024-12-04},
	date = {2021-07-21},
	langid = {english},
}

@inproceedings{engelmann_blacklists_2021-1,
	location = {Virtual Event {USA}},
	title = {Blacklists and Redlists in the Chinese Social Credit System: Diversity, Flexibility, and Comprehensiveness},
	isbn = {978-1-4503-8473-5},
	url = {https://dl.acm.org/doi/10.1145/3461702.3462535},
	doi = {10.1145/3461702.3462535},
	shorttitle = {Blacklists and Redlists in the Chinese Social Credit System},
	abstract = {The social credit system is a nationwide reputation system of the People’s Republic of China to evaluate its citizens and enterprises. The goal is to increase the trust in the economy and society through the use of big data technology, artiﬁcial intelligence and public surveillance. Therefore, the government launched a system of Blacklists and Redlists to distinguish between trustworthy and untrustworthy parties and to sanction or reward natural persons and enterprises. Since the Chinese government plans, to ofﬁcially launch the nationwide system by 2020 [1] in a uniform scheme, the objective of this bachelor’s thesis was to elaborate the progress of the social credit system on the basis of its online appearance. In particular, the differences of Blacklists and Redlists among the four municipalities, ﬁve autonomous regions and 22 provinces of China were framed. For this task, a data-driven analysis approach was conducted. The multiple social credit websites were inspected with regard to the Blacklists and Redlists. Furthermore, a reusable data collection tool was developed to collect Blacklist and Redlist sample entries. With the tool, a dataset of 75677 list entries was created and analyzed. The results showed large variability in the implementation of Blacklists and Redlists, as well as high heterogeneity in depth of information among all 31 administrative divisions.},
	eventtitle = {{AIES} '21: {AAAI}/{ACM} Conference on {AI}, Ethics, and Society},
	pages = {78--88},
	booktitle = {Proceedings of the 2021 {AAAI}/{ACM} Conference on {AI}, Ethics, and Society},
	publisher = {{ACM}},
	author = {Engelmann, Severin and Chen, Mo and Dang, Lorenz and Grossklags, Jens},
	urldate = {2024-12-04},
	date = {2021-07-21},
	langid = {english},
}

