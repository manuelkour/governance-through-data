@article{liang_constructing_2018,
	title = {Constructing a Data-Driven Society: China's Social Credit System as a State Surveillance Infrastructure},
	volume = {10},
	issn = {1944-2866},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/poi3.183},
	doi = {10.1002/poi3.183},
	shorttitle = {Constructing a Data-Driven Society},
	abstract = {Big data technologies have been adopted by both the public and private sectors to develop and expand surveillance capacities. This article traces the institutional processes and political-economic interests of the public and private stakeholders involved in the construction of China's Social Credit System ({SCS}), which is currently on track for full deployment on 1.4 billion citizens by 2020. The {SCS} aims to centralize data platforms into a big data–enabled surveillance infrastructure to manage, monitor, and predict the trustworthiness of citizens, firms, organizations, and governments in China. A punishment/reward system based on credit scores will determine whether citizens and organizations are able to access things like education, markets, and tax deductions. While the {SCS} is widely described by the Western news media as a means of “big brother” or political control, we find that it is a complicated system that focuses primarily on financial and commercial activities rather than political ones. This article presents a framework for understanding state surveillance infrastructures by exploring how various government agencies are cooperating to establish this centralized data infrastructure with the aim of scoring credit, and discussing the distinct but interconnected processes of data collection, data aggregation, and data analytics.},
	pages = {415--453},
	number = {4},
	journaltitle = {Policy \& Internet},
	author = {Liang, Fan and Das, Vishnupriya and Kostyuk, Nadiya and Hussain, Muzammil M.},
	urldate = {2025-01-19},
	date = {2018},
	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/poi3.183},
	keywords = {big data, data platforms, infrastructures, social credit, state surveillance},
}

