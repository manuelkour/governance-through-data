@article{chen_exploring_2024,
	title = {Exploring the microscopic mechanism of credit repair knowledge dissemination: A complex network-based approach},
	volume = {238},
	issn = {0957-4174},
	url = {https://www.sciencedirect.com/science/article/pii/S0957417423023254},
	doi = {10.1016/j.eswa.2023.121823},
	shorttitle = {Exploring the microscopic mechanism of credit repair knowledge dissemination},
	abstract = {The micro-evolutionary mechanisms of credit repair ({CR}) knowledge dissemination among the public is crucial for the design of {CR} mechanisms and the healthy growth of China's social credit environment. However, previous research has seldom focused on {CR} knowledge dissemination. Knowledge dissemination would be influenced by knowledge benefits, government incentives, dissemination costs, and social credit risk. To study the impact of these factors on {CR} knowledge dissemination, we built an evolutionary game model of {CR} knowledge dissemination based on complex networks. This model includes agents as network nodes with different degrees of credit knowledge and a willingness to share information. Through numerical simulations, this study examines the impact of these factors on {CR} knowledge dissemination, demonstrating that improving the public's learning abilities is a powerful and effective way to promote {CR} knowledge dissemination. Compared with other influencing factors, the promotional effect of learning ability was more prominent. To ensure public motivation for knowledge dissemination, dissemination costs must be maintained at low levels. Additionally, the risk to the social credit environment has a significant impact on the effectiveness of dissemination, and high credit environment risk can hinder the dissemination of credit knowledge. Particularly, a high-risk environment can easily lead to the failure of government incentives.},
	pages = {121823},
	journaltitle = {Expert Systems with Applications},
	shortjournal = {Expert Systems with Applications},
	author = {Chen, Weijie and Zhang, Tao and Liu, Yuanqing and Tang, Yuxiu},
	urldate = {2025-04-14},
	date = {2024-03-15},
	keywords = {Agent-based model, Complex network, Credit repair, Evolutionary dynamics, Knowledge dissemination},
}

@article{tsai_hobbling_2021,
	title = {Hobbling Big Brother: Top-Level Design and Local Discretion in China’s Social Credit System},
	volume = {86},
	issn = {1324-9347, 1835-8535},
	url = {https://www.journals.uchicago.edu/doi/10.1086/714492},
	doi = {10.1086/714492},
	shorttitle = {Hobbling Big Brother},
	abstract = {This article argues that policy implementation in China has moved toward a system under which the center formulates a blueprint or “top-level design” from which local ofﬁcials devise detailed regulations through a process of “crossing the river by feeling for stones.” This applies to the development of China’s new social credit system. To minimize political risk, when implementing it local ofﬁcials have selected easier goals from the “top-level design” blueprint. In City Z, for example, we ﬁnd that promoting integrity among businesses is less risky than promoting integrity among civil servants or the general public, so less progress has been made in the latter two areas when putting in place integrity credit-point systems. The risks involved in promoting social credit among the general populace are the greatest, so the city’s efforts in this have been limited. From our empirical study of China’s social credit system in City Z, we draw some conclusions about policy implementation under Xi Jinping.},
	pages = {1--20},
	journaltitle = {The China Journal},
	shortjournal = {The China Journal},
	author = {Tsai, Wen-Hsuan and Wang, Hsin-Hsien and Lin, Ruihua},
	urldate = {2025-02-23},
	date = {2021-07-01},
	langid = {english},
}

