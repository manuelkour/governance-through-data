@article{serrano_oswald_techno-regulation_2022,
	title = {Techno-Regulation in China: How are Bavarian Firms Faring Under the Social Credit System?},
	rights = {Creative Commons Attribution 4.0 International},
	url = {https://en.bidt.digital/publication/technoregulation-in-china-bavarian-companies-in-the-social-credit-system/},
	doi = {10.35067/XYPQ-KN21},
	shorttitle = {Techno-Regulation in China},
	abstract = {Since introduction of the Chinese Social Credit System ({SCS}) in 2014 – 2020, there have
been concerns about the repercussions of this system on companies and individuals trading in China. In this study, we examine the Chinese {SCS}’ impact on Bavarian companies. We investigate how large, medium, small and micro Bavarian companies active in China are being classified and assessed under the {SCS}. This publicly accessible system aims to rank specific business activities either as desirable, rewarding them (through red lists), or as undesirable, punishing them (through administrative penalties or blacklists).

Our analysis of 170 Bavarian companies in China shows that these firms, for the most part, feature on red (positive) lists. However, almost 9\% of the companies are subject to negative entries in the system by way of an administrative penalty, which under certain circumstances may lead to inclusion on a blacklist. The positive entries relate mainly to tax matters, while contraventions to regulations in the areas of work safety, health and environment constitute the majority of negative entries. At regional level, though, there are significant differences regarding the implementation of the {SCS} by local authorities. We complement our analysis with findings from 10 in-depth interviews that provide insights into the experiences and perspectives of Bavarian companies based in China in the context of the {SCS}. Our evaluation feeds into recommendations for action, particularly regarding potential measures of support for Bavarian companies with Chinese subsidiaries.},
	pages = {26 pages},
	author = {Serrano Oswald, Omar Ramon and Selmanovic, Sandra and Lee, Benjamin and Arndt, Laura},
	urldate = {2025-02-27},
	date = {2022},
	langid = {english},
	note = {Artwork Size: 26 pages
Medium: pdf
Publisher: Bavarian Research Institute for Digital Transformation (bidt)},
}

